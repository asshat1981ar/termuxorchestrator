# Example keystore.txt format
# Copy this to keystore.txt and fill in your actual values

# GitHub credentials
GITHUB_TOKEN=github_pat_your_token_here
GITHUB_USERNAME=your_github_username

# AI API Keys
OPENAI_API_KEY=sk-your_openai_key_here
ANTHROPIC_API_KEY=ak-your_anthropic_key_here
GOOGLE_API_KEY=your_google_key_here
DEEPSEEK_API_KEY=your_deepseek_key_here

# Expo/EAS credentials
EXPO_TOKEN=your_expo_token_here

# Codemagic credentials
CODEMAGIC_ACCESS_TOKEN=your_codemagic_token_here
CODEMAGIC_PROJECT_ID=your_project_id_here

# Android keystore credentials
KEYSTORE_PASSWORD=your_keystore_password
KEY_PASSWORD=your_key_password
KEY_ALIAS=your_key_alias
KEYSTORE_PATH=/path/to/your/keystore.jks

# Google Play credentials (optional)
GOOGLE_PLAY_JSON=/path/to/service-account.json